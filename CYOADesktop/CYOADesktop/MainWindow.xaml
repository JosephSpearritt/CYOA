<Window x:Class="CYOADesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CYOADesktop"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnTestWrite" Content="Test Write" HorizontalAlignment="Left" Margin="99,121,0,0" VerticalAlignment="Top" Width="75" Click="btnTestWrite_Click"/>
        <Button x:Name="btnTestRead" Content="Test Read" HorizontalAlignment="Left" Margin="199,121,0,0" VerticalAlignment="Top" Width="75" Click="btnTestRead_Click"/>

    </Grid>
</Window>
